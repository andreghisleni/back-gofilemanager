{
  "name": "backend-goagenda",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "ts-node-dev --no-notify -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "dev:queue": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/shared/infra/queue/run.ts",
    "dev:bull": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/shared/infra/queue/bullboard.ts",
    "start": "ts-node  src/shared/infra/http/server.ts",
    "start:prod": "node  dist/shared/infra/http/server.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed:run": "ts-node -r tsconfig-paths/register src/shared/infra/typeorm/seeds",
    "test": "majestic",
    "test:jest": "jest",
    "openCoverage": "sensible-browser ./coverage/lcov-report/index.html",
    "lint": "eslint -c ./.eslintrc.json ./src/**/*{.ts,.tsx} --fix --format=tap"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/geoip-lite": "^1.4.1",
    "@types/hapi__joi": "^17.1.8",
    "@types/html-minifier": "^4.0.2",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mime": "^3.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.5",
    "@types/redis": "^4.0.11",
    "@types/showdown": "^2.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.0.3",
    "majestic": "^1.8.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3",
    "yarn-upgrade-all": "^0.7.1"
  },
  "dependencies": {
    "@bull-board/api": "^4.2.2",
    "@bull-board/express": "^4.2.2",
    "aws-sdk": "^2.1213.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.9.0",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "geoip-lite": "^1.4.6",
    "handlebars": "^4.7.7",
    "html-minifier": "^4.0.0",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "mongodb": "^4.9.1",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "opencage-api-client": "^1.0.0",
    "pg": "^8.8.0",
    "rate-limiter-flexible": "^2.3.9",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "showdown": "^2.1.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.9",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "majestic": {
    "jestScriptPath": "./node_modules/jest/bin/jest.js",
    "args": [
      "--config=./jest.config.ts"
    ],
    "env": {
      "CI": "true"
    }
  }
}
